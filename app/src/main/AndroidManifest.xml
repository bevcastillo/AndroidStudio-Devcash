<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.devcash">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AddCategoryActivity"
            android:label="@string/title_addcat"
            android:screenOrientation="landscape" />
        <activity
            android:name=".AddDiscountActivity"
            android:label="@string/title_adddisc"
            android:screenOrientation="landscape" />
        <activity
            android:name=".AddEmployeeActivity"
            android:label="@string/title_addemp"
            android:screenOrientation="landscape" />
        <activity
            android:name=".AddServicesActivity"
            android:label="@string/title_addservice"
            android:screenOrientation="landscape" />
        <activity
            android:name=".AddProductActivity"
            android:label="@string/title_addprod"
            android:screenOrientation="landscape" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/menu_settings"
            android:screenOrientation="landscape" />
        <activity
            android:name=".NavActivity"
            android:label="Navagation Activity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OwnerLogin"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>